-- Таблица: Студенты
DROP TABLE IF EXISTS Студенты;
CREATE TABLE IF NOT EXISTS Студенты (
    студент_id      INTEGER PRIMARY KEY,
    ФИО             TEXT    NOT NULL,
    пол             TEXT,
    дата_рождения   DATE,
    номер_аттестата TEXT,
    группа_id       INTEGER,
    FOREIGN KEY (группа_id) REFERENCES Группы(группа_id)
);
